{
  "name": "json-size-explorer",
  "version": "1.0.3",
  "scripts": {
    "ui:start": "react-scripts start",
    "ui:build": "react-scripts build",
    "build": "rollup -c",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d ui:build",
    "test": "node node_modules/.bin/jest",
    "test:watch": "node node_modules/.bin/jest --watch",
    "test:coverage": "node node_modules/.bin/jest --coverage",
    "test:coverage:ci": "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "coveralls": "3.0.2",
    "jest": "^23.1.0",
    "prettier": "1.17.0",
    "rollup": "^0.60.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "typescript": "3.4.4"
  },
  "dependencies": {
    "cli-table3": "^0.5.0",
    "commander": "^2.15.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.10",
    "thousands": "^1.0.1",
    "update-notifier": "^2.5.0"
  },
  "description": "Get insight of the size of your JSON documents",
  "keywords": [
    "json",
    "size",
    "explorer"
  ],
  "homepage": "http://everymscount.com/json-size-explorer/",
  "author": "Thomas Genin <xt6.thomas.genin@gmail.com>",
  "repository": "https://github.com/genintho/json-size-explorer.git",
  "license": "MIT",
  "main": "dist/json-size-explorer.cjs.js",
  "module": "dist/json-size-explorer.esm.js",
  "browser": "dist/json-size-explorer.js",
  "browser.min": "dist/json-size-explorer.min.js",
  "bin": {
    "json-size-explorer": "./index.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "tabWidth": 4
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
