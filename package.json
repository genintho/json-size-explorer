{
  "name": "json-size-explorer",
  "version": "1.0.3",
  "scripts": {
    "build": "rollup -c",
    "test": "node node_modules/.bin/jest",
    "test:watch": "node node_modules/.bin/jest --watch",
    "test:coverage": "node node_modules/.bin/jest --coverage",
    "test:coverage:ci": "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash": "^4.14.123",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "coveralls": "3.0.2",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "1.17.0",
    "rollup": "^0.60.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "cli-table3": "^0.5.0",
    "commander": "^2.15.1",
    "lodash": "^4.17.10",
    "thousands": "^1.0.1",
    "typescript": "3.4.4",
    "update-notifier": "^2.5.0"
  },
  "description": "Get insight of the size of your JSON documents",
  "keywords": [
    "json",
    "size",
    "explorer"
  ],
  "author": "Thomas Genin <xt6.thomas.genin@gmail.com>",
  "repository": "https://github.com/genintho/json-size-explorer.git",
  "license": "MIT",
  "main": "dist/json-size-explorer.cjs.js",
  "module": "dist/json-size-explorer.esm.js",
  "browser": "dist/json-size-explorer.js",
  "browser.min": "dist/json-size-explorer.min.js",
  "bin": {
    "json-size-explorer": "./index.js"
  },
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "tabWidth": 4
  }
}
