{
  "name": "json-size-explorer",
  "version": "1.0.3",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "node node_modules/.bin/jest",
    "test:watch": "node node_modules/.bin/jest --watch",
    "test:coverage": "node node_modules/.bin/jest --coverage",
    "test:coverage:ci": "node node_modules/.bin/jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/lodash": "4.14.126",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "coveralls": "3.0.3",
    "jest": "24.7.1",
    "lodash": "4.17.11",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "thousands": "1.0.1",
    "typescript": "5.4.3"
  },
  "description": "Get insight of the size of your JSON documents",
  "keywords": [
    "json",
    "size",
    "explorer"
  ],
  "homepage": "http://everymscount.com/json-size-explorer/",
  "author": "Thomas Genin <xt6.thomas.genin@gmail.com>",
  "repository": "https://github.com/genintho/json-size-explorer.git",
  "license": "MIT",
  "prettier": {
    "trailingComma": "es5",
    "arrowParens": "always",
    "tabWidth": 4
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "gh-pages": "^2.0.1"
  }
}
